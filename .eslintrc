{
  "parser": "babel-eslint",
  "plugins": [
      "html",
      "flowtype"
  ],
  "extends": [
      "eslint:recommended",
      "plugin:flowtype/recommended",
      "vue"
  ],
  "rules": {
      "indent": 0,
      "semi": 0,
      "camelcase": 0,
      "no-console": 0,
      "space-before-function-paren": 0,
      "keyword-spacing": 0,
      "arrow-parens": 0,
      "eol-last": 0,
      "prefer-const": 0,
      "no-trailing-spaces": 0,
      "space-in-parens": 0,
      "comma-dangle": 0
  }
}